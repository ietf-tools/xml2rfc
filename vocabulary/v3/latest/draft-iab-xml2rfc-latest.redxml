   <t anchor="element.postamble.contents"><!--AG-->
      Content model:
    </t>
   <t><!--AG-->In any order: <list style="symbols">
         <t><!--AG-->Text</t>
         <t><!--AG-->
            <iref item="Elements" subitem="bcp14"/>
