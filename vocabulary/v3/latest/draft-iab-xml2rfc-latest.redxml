         <t><!--AG-->
            <iref item="Elements" subitem="sub"/>
            <iref item="sub element" subitem="inside postamble"/>&lt;<xref target="element.sub" format="none">sub</xref>&gt; elements (<xref target="element.sub"/>)</t>
