   <t anchor="element.dl.contents"><!--AG-->
      Content model:
    </t>
   <t><!--AG-->
    One or more sequences of:
  </t>
   <t><list style="numbers"><!--AG-->
        
      <t><!--AG-->
         <iref item="Elements" subitem="dt"/>
         <iref item="dt element" subitem="inside dl"/>One &lt;<xref target="element.dt" format="none">dt</xref>&gt; element</t>
        
      <t><!--AG-->
         <iref item="Elements" subitem="dd"/>
         <iref item="dd element" subitem="inside dl"/>One &lt;<xref target="element.dd" format="none">dd</xref>&gt; element</t>
      
   </list></t>

   <!--dl/@anchor-->
   <section anchor="element.dl.attribute.anchor" toc="exclude" title="&#34;anchor&#34; Attribute">
      
      <iref item="Attributes" subitem="anchor"/>
